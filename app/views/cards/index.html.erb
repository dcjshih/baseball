<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl flex p-10">Meet the Team</h1>
  </div>

  <div class="grid grid-cols-2 gap-10 flex justify-items-center py-20 bg-[#042c54] rounded-lg">
      <%= render @cards %>
  <div>
</div>
